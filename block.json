{
	"apiVersion": "2",	
	"name": "wphackers/qr-block",
	"title": "QR Block",
	"category": "widgets",
	"description": "Create a QR code.",
	"supports": {
		"html": false,
		"align": true,
		"media-manager/color": {
			"code": {
				"label": "Code color",
				"style": "none"
			},
			"background": {
				"label": "Background color",
				"style": "none"
			},
			"__contrastChecker": [ "code", "background" ],
			"__sectionTitle": "Color"
		}
	},
	"attributes": {
		"value": {
			"type": "string",
			"default": "Say Hello to the New Editor! https://wordpress.org/gutenberg/. AKA Gutenlove ðŸ’–"
		},
		"size": {
			"type": "integer",
			"default": 1
		},
		"level": {
			"type": "string",
			"default": "L"
		},
		"codeHEXColor": {
			"type": "string"
		},
		"bgHEXColor": {
			"type": "string"
		}
	},
	"textdomain": "qr-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}