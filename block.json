{
	"apiVersion": 2,
	"name": "create-block/qr-block",
	"version": "0.0.1",
	"title": "QR Block",
	"category": "widgets",
	"description": "Create a QR code.",
	"supports": {
		"html": false,
		"align": true,
		"media-manager/color": {
			"code": {
				"label": "Code color",
				"style": "none"
			},
			"background": {
				"label": "Background color",
				"style": "none"
			},
			"__contrastChecker": [ "code", "background" ],
			"__sectionTitle": "Color"
		}
	},
	"attributes": {
		"value": {
			"type": "string",
			"default": "Visit https://www.wordpress.org"
		},
		"size": {
			"type": "integer",
			"default": 1
		},
		"level": {
			"type": "string",
			"default": "L"
		},
		"codeHEXColor": {
			"type": "string"
		},
		"bgHEXColor": {
			"type": "string"
		}
	},
	"textdomain": "qr-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"sizes": [
		{
			"label": "Small",
			"slug": "small",
			"value": 1
		},
		{
			"label": "Medium",
			"slug": "medium",
			"value": 2
		},
		{
			"label": "Large",
			"slug": "large",
			"value": 3
		}
	]
}