{
	"apiVersion": 2,
	"name": "create-block/qr-block",
	"version": "0.0.1",
	"title": "QR Block",
	"category": "widgets",
	"description": "Create a QR code.",
	"supports": {
		"media-manager/sizes": {
			"options": [
				{
					"name": "Small",
					"slug": "small",
					"key": "key-small",
					"size": 1
				},
				{
					"name": "Medium",
					"slug": "normal",
					"key": "key-normal",
					"size": 2
				},
				{
					"name": "Large",
					"slug": "large",
					"key": "key-large",
					"size": 3
				}
			]
		},
		"html": false,
		"align": true,
		"media-manager/color": {
			"code": {
				"label": "Code color",
				"style": "none"
			},
			"background": {
				"label": "Background color",
				"style": "none"
			},
			"__contrastChecker": [ "code", "background" ],
			"__sectionTitle": "Color"
		}
	},
	"attributes": {
		"value": {
			"type": "string",
			"default": "Visit https://www.wordpress.org"
		},
		"level": {
			"type": "string",
			"default": "L"
		},
		"codeHEXColor": {
			"type": "string"
		},
		"bgHEXColor": {
			"type": "string"
		}
	},
	"textdomain": "qr-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}