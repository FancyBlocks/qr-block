{
	"apiVersion": "2",	
	"name": "wphackers/qr-post",
	"title": "QR Post",
	"category": "widgets",
	"description": "Share your post with a QR Code.",
	"supports": {
		"html": false,
		"align": true,
		"media-manager/color": {
			"code": {
				"label": "Code color",
				"style": "none"
			},
			"background": {
				"label": "Background color",
				"style": "none"
			},
			"__contrastChecker": [ "code", "background" ],
			"__sectionTitle": "Color"
		}
	},
	"attributes": {
		"size": {
			"type": "integer",
			"default": 200
		},
		"level": {
			"type": "string",
			"default": "L"
		},
		"codeHEXColor": {
			"type": "string"
		},
		"bgHEXColor": {
			"type": "string"
		}
	},
	"textdomain": "qr-post",
	"editorScript": "file:../../../build/index.js",
	"editorStyle": "file:../../../build/index.css",
	"style": "file:../../../build/style-index.css"
}